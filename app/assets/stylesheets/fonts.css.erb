@charset "UTF-8";

/*DOSIS*/
@font-face {
  font-family: Dosis;
  src: url(<%= asset_path 'Dosis-Regular.ttf' %>);
  font-weight: normal;
  font-style: normal;
}

/*ICONS*/
@font-face {
  font-family: "b2b";
  src:url(<%= asset_path "b2b.eot" %>);
  src:url(<%= asset_path "b2b.eot?#iefix" %>) format("embedded-opentype"),
    url(<%= asset_path "b2b.woff" %>) format("woff"),
    url(<%= asset_path "b2b.ttf" %>) format("truetype"),
    url(<%= asset_path "b2b.svg#b2b" %>) format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "b2b" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "b2b" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  vertical-align: middle;
}


